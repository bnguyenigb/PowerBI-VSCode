{
	"name": "powerbi-vscode",
	"displayName": "Power BI VSCode",
	"description": "Power BI Extension for VSCode",
	"version": "0.0.3",
	"publisher": "GerhardBrueckl",
	"icon": "resources/powerbi_extension.png",
	"author": {
		"name": "Gerhard Brueckl"
	},
	"license": "MIT",
	"homepage": "https://github.com/gbrueckl/PowerBI-VSCode/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/gbrueckl/PowerBI-VSCode"
	},
	"bugs": {
		"url": "https://github.com/gbrueckl/PowerBI-VSCode/issues",
		"email": "powerbi-vscode@gbrueckl.at"
	},
	"qna": "https://github.com/gbrueckl/PowerBI-VSCode",
	"engines": {
		"vscode": "^1.66.0"
	},
	"galleryBanner": {
		"color": "#0ea5ff",
		"theme": "dark"
	},
	"categories": [
		"Azure",
		"Extension Packs",
		"Other"
	],
	"keywords": [
		"power bi",
		"rest",
		"api"
	],
	"activationEvents": [
		"onView:PowerBIWorkspaces",
		"onView:PowerBICapacities",
		"onView:PowerBIGateways"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": [],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "powerbi",
					"title": "Power BI",
					"icon": "resources/powerbi_sidebar.png"
				}
			]
		},
		"views": {
			"powerbi": [
				{
					"id": "PowerBIWorkspaces",
					"name": "Workspaces"
				},
				{
					"id": "PowerBICapacities",
					"name": "Capacities"
				},
				{
					"id": "PowerBIGateways",
					"name": "Gateways"
				},
				{
					"id": "PowerBIPipelines",
					"name": "Pipelines"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "PowerBIWorkspaces",
				"contents": "Please log in using Azure Account Extension -> Sign In"
			},
			{
				"view": "PowerBICapacities",
				"contents": "Please log in using Azure Account Extension -> Sign In"
			},
			{
				"view": "PowerBIGateways",
				"contents": "Please log in using Azure Account Extension -> Sign In"
			},
			{
				"view": "PowerBIPipelines",
				"contents": "Please log in using Azure Account Extension -> Sign In"
			}
		],
		"commands": [
			{
				"command": "PowerBI.updateQuickPickList",
				"title": "Update Quick Pick List"
			},
			{
				"command": "PowerBIWorkspaces.refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.png",
					"dark": "resources/dark/refresh.png"
				}
			},
			{
				"command": "PowerBIWorkspaces.add",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/add.png",
					"dark": "resources/dark/add.png"
				}
			},
			{
				"command": "PowerBIWorkspace.delete",
				"title": "Delete",
				"icon": {
					"light": "resources/light/delete.png",
					"dark": "resources/dark/delete.png"
				}
			},
			{
				"command": "PowerBIDataset.delete",
				"title": "Delete",
				"icon": {
					"light": "resources/light/delete.png",
					"dark": "resources/dark/delete.png"
				}
			},
			{
				"command": "PowerBIDataset.refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.png",
					"dark": "resources/dark/refresh.png"
				}
			},
			{
				"command": "PowerBIDataset.takeOver",
				"title": "Take Over",
				"icon": {
					"light": "resources/light/disconnected.png",
					"dark": "resources/dark/disconnected.png"
				}
			},
			{
				"command": "PowerBIReport.delete",
				"title": "Delete",
				"icon": {
					"light": "resources/light/delete.png",
					"dark": "resources/dark/delete.png"
				}
			},
			{
				"command": "PowerBIReport.clone",
				"title": "Clone",
				"icon": {
					"light": "resources/light/copy.png",
					"dark": "resources/dark/copy.png"
				}
			},
			{
				"command": "PowerBIReport.rebind",
				"title": "Rebind",
				"icon": {
					"light": "resources/light/rebind.png",
					"dark": "resources/dark/rebind.png"
				}
			},
			{
				"command": "PowerBIReport.updateContent",
				"title": "Update Content",
				"icon": {
					"light": "resources/light/updateContent.png",
					"dark": "resources/dark/updateContent.png"
				}
			},
			{
				"command": "PowerBIDataflow.delete",
				"title": "Delete",
				"icon": {
					"light": "resources/light/delete.png",
					"dark": "resources/dark/delete.png"
				}
			},
			{
				"command": "PowerBIDataflow.refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.png",
					"dark": "resources/dark/refresh.png"
				}
			},
			{
				"command": "PowerBIDashboard.delete",
				"title": "Delete",
				"icon": {
					"light": "resources/light/delete.png",
					"dark": "resources/dark/delete.png"
				}
			},
			{
				"command": "PowerBICapacities.refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.png",
					"dark": "resources/dark/refresh.png"
				}
			},
			{
				"command": "PowerBIGateways.refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.png",
					"dark": "resources/dark/refresh.png"
				}
			},
			{
				"command": "PowerBIPipelines.refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.png",
					"dark": "resources/dark/refresh.png"
				}
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "PowerBIWorkspaces.refresh",
					"when": "view == PowerBIWorkspaces",
					"group": "navigation"
				},
				{
					"command": "PowerBIWorkspaces.add",
					"when": "view == PowerBIWorkspaces",
					"group": "navigation"
				},
				{
					"command": "PowerBICapacities.refresh",
					"when": "view == PowerBICapacities",
					"group": "navigation"
				},
				{
					"command": "PowerBIGateways.refresh",
					"when": "view == PowerBIGateways",
					"group": "navigation"
				},
				{
					"command": "PowerBIPipelines.refresh",
					"when": "view == PowerBIPipelines",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "PowerBIWorkspace.delete",
					"when": "view == PowerBIWorkspaces && viewItem == GROUP",
					"group": "inline"
				},
				{
					"command": "PowerBIDataset.delete",
					"when": "view == PowerBIWorkspaces && viewItem == DATASET",
					"group": "inline"
				},
				{
					"command": "PowerBIDataset.refresh",
					"when": "view == PowerBIWorkspaces && viewItem == DATASET",
					"group": "inline"
				},
				{
					"command": "PowerBIDataset.takeOver",
					"when": "view == PowerBIWorkspaces && viewItem == DATASET"
				},
				{
					"command": "PowerBIReport.delete",
					"when": "view == PowerBIWorkspaces && viewItem == REPORT",
					"group": "inline"
				},
				{
					"command": "PowerBIReport.clone",
					"when": "view == PowerBIWorkspaces && viewItem == REPORT"
				},
				{
					"command": "PowerBIReport.rebind",
					"when": "view == PowerBIWorkspaces && viewItem == REPORT"
				},
				{
					"command": "PowerBIReport.updateContent",
					"when": "view == PowerBIWorkspaces && viewItem == REPORT"
				},
				{
					"command": "PowerBIDataflow.delete",
					"when": "view == PowerBIWorkspaces && viewItem == DATAFLOW",
					"group": "inline"
				},
				{
					"command": "PowerBIDataflow.refresh",
					"when": "view == PowerBIWorkspaces && viewItem == DATAFLOW",
					"group": "inline"
				},
				{
					"command": "PowerBIDashboard.delete",
					"when": "view == PowerBIWorkspaces && viewItem == DASHBOARD",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run esbuild-base -- --minify --external:keytar",
		"esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
		"esbuild": "npm run esbuild-base -- --sourcemap --external:keytar",
		"esbuild-watch": "npm run esbuild-base -- --sourcemap --watch --external:keytar",
		"test-compile": "tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/mkdirp": "^0.5.2",
		"@types/node": "^12.12.0",
		"@types/rimraf": "^2.0.5",
		"@types/vscode": "^1.66.0",
		"esbuild": "^0.14.43",
		"ts-loader": "^8.3.0",
		"tslint": "^5.20.1",
		"typescript": "^4.4.3",
		"webpack": "^5.62.1",
		"webpack-cli": "^4.9.1"
	},
	"dependencies": {
		"@angular/common": "^14.0.1",
		"@angular/core": "^14.0.1",
		"@azure/identity": "^2.0.4",
		"@azure/identity-vscode": "^1.0.0",
		"axios": "^0.21.4",
		"form-data": "^4.0.0",
		"https-agent": "^1.0.0",
		"jsonc-parser": "^0.4.2"
	}
}
