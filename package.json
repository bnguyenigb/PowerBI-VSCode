{
	"name": "powerbi-vscode",
	"displayName": "Power BI VSCode",
	"description": "Power BI Extension for VSCode",
	"version": "0.3.0",
	"publisher": "GerhardBrueckl",
	"icon": "resources/powerbi_extension.png",
	"author": {
		"name": "Gerhard Brueckl"
	},
	"license": "MIT",
	"homepage": "https://github.com/gbrueckl/PowerBI-VSCode/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/gbrueckl/PowerBI-VSCode"
	},
	"bugs": {
		"url": "https://github.com/gbrueckl/PowerBI-VSCode/issues",
		"email": "powerbi-vscode@gbrueckl.at"
	},
	"qna": "https://github.com/gbrueckl/PowerBI-VSCode",
	"engines": {
		"vscode": "^1.68.0"
	},
	"galleryBanner": {
		"color": "#0ea5ff",
		"theme": "dark"
	},
	"categories": [
		"Azure",
		"Extension Packs",
		"Other"
	],
	"keywords": [
		"Power BI",
		"Rest",
		"API",
		"Reporting",
		"Management",
		"PowerBI"
	],
	"activationEvents": [
		"onView:PowerBIWorkspaces",
		"onView:PowerBICapacities",
		"onView:PowerBIGateways",
		"onView:PowerBIPipelines"
	],
	"main": "./dist/node/extension.js",
	"browser": "./dist/web/extension.js",
	"contributes": {
		"configuration": [
			{
				"title": "Power BI",
				"properties": {
					"powerbi.tenantId": {
						"type": "string",
						"description": "(Optional) A custom TenantID to connect to a remote Tenant where you are a guest user."
					},
					"powerbi.clientId": {
						"type": "string",
						"description": "(Optional) A custom ClientID/Application of an AAD application to use when connecting to Power BI."
					},
					"powerbi.apiUrl": {
						"type": "string",
						"description": "The URL of the Power BI API. Default is https://api.powerbi.com/",
						"default": "https://api.powerbi.com/"
					}
				}
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "powerbi",
					"title": "Power BI",
					"icon": "resources/powerbi_sidebar.png"
				}
			]
		},
		"views": {
			"powerbi": [
				{
					"id": "PowerBIWorkspaces",
					"name": "Workspaces"
				},
				{
					"id": "PowerBICapacities",
					"name": "Capacities"
				},
				{
					"id": "PowerBIGateways",
					"name": "Gateways"
				},
				{
					"id": "PowerBIPipelines",
					"name": "Pipelines"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "PowerBIWorkspaces",
				"contents": "Please log in to VSCode with a Microsoft Account first!"
			},
			{
				"view": "PowerBICapacities",
				"contents": "Please log in to VSCode with a Microsoft Account first!"
			},
			{
				"view": "PowerBIGateways",
				"contents": "Please log in to VSCode with a Microsoft Account first!"
			},
			{
				"view": "PowerBIPipelines",
				"contents": "Please log in to VSCode with a Microsoft Account first!"
			}
		],
		"commands": [
			{
				"command": "PowerBI.updateQuickPickList",
				"title": "Update Quick Pick List"
			},
			{
				"command": "PowerBIWorkspaces.refresh",
				"title": "Refresh",
				"icon": "$(refresh)"
			},
			{
				"command": "PowerBIWorkspaces.add",
				"title": "Add workspace",
				"icon": "$(add)"
			},
			{
				"command": "PowerBIWorkspace.delete",
				"title": "Delete",
				"icon": "$(remove-close)"
			},
			{
				"command": "PowerBIDataset.delete",
				"title": "Delete",
				"icon": "$(remove-close)"
			},
			{
				"command": "PowerBIDataset.refresh",
				"title": "Refresh",
				"icon": "$(refresh)"
			},
			{
				"command": "PowerBIDataset.cancelRefresh",
				"title": "Cancel",
				"icon": "$(stop)"
			},
			{
				"command": "PowerBIDataset.createKernel",
				"title": "Create Notebook Kernel",
				"icon": "$(notebook)"
			},
			{
				"command": "PowerBIDataset.removeKernel",
				"title": "Remove Notebook Kernel",
				"icon": "$(notebook-template)"
			},
			{
				"command": "PowerBIDataset.takeOver",
				"title": "Take Over",
				"icon": {
					"light": "resources/light/disconnected.png",
					"dark": "resources/dark/disconnected.png"
				}
			},
			{
				"command": "PowerBIDataset.updateAllParameters",
				"title": "Edit Parameters",
				"icon": "$(pencil)"
			},
			{
				"command": "PowerBIDatasetParameter.update",
				"title": "Edit",
				"icon": "$(pencil)"
			},
			{
				"command": "PowerBIReport.takeOver",
				"title": "Take Over",
				"icon": {
					"light": "resources/light/disconnected.png",
					"dark": "resources/dark/disconnected.png"
				}
			},
			{
				"command": "PowerBIReport.delete",
				"title": "Delete",
				"icon": "$(remove-close)"
			},
			{
				"command": "PowerBIReport.clone",
				"title": "Clone",
				"icon": "$(copy)"
			},
			{
				"command": "PowerBIReport.rebind",
				"title": "Rebind",
				"icon": {
					"light": "resources/light/rebind.png",
					"dark": "resources/dark/rebind.png"
				}
			},
			{
				"command": "PowerBIReport.updateContent",
				"title": "Update Content",
				"icon": {
					"light": "resources/light/updateContent.png",
					"dark": "resources/dark/updateContent.png"
				}
			},
			{
				"command": "PowerBIDataflow.delete",
				"title": "Delete",
				"icon": "$(remove-close)"
			},
			{
				"command": "PowerBIDataflow.refresh",
				"title": "Refresh",
				"icon": "$(refresh)"
			},
			{
				"command": "PowerBIDashboard.delete",
				"title": "Delete",
				"icon": "$(remove-close)"
			},
			{
				"command": "PowerBICapacities.refresh",
				"title": "Refresh",
				"icon": "$(refresh)"
			},
			{
				"command": "PowerBIGateways.refresh",
				"title": "Refresh",
				"icon": "$(refresh)"
			},
			{
				"command": "PowerBIPipelines.refresh",
				"title": "Refresh",
				"icon": "$(refresh)"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "PowerBIWorkspaces.refresh",
					"when": "view == PowerBIWorkspaces",
					"group": "navigation"
				},
				{
					"command": "PowerBIWorkspaces.add",
					"when": "view == PowerBIWorkspaces",
					"group": "navigation"
				},
				{
					"command": "PowerBICapacities.refresh",
					"when": "view == PowerBICapacities",
					"group": "navigation"
				},
				{
					"command": "PowerBIGateways.refresh",
					"when": "view == PowerBIGateways",
					"group": "navigation"
				},
				{
					"command": "PowerBIPipelines.refresh",
					"when": "view == PowerBIPipelines",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "PowerBIWorkspace.delete",
					"when": "view == PowerBIWorkspaces && viewItem =~ /.*,GROUP,.*/",
					"group": "inline"
				},
				{
					"command": "PowerBIDataset.delete",
					"when": "view == PowerBIWorkspaces && viewItem =~ /.*,DATASET,.*/",
					"group": "inline"
				},
				{
					"command": "PowerBIDataset.refresh",
					"when": "view == PowerBIWorkspaces && viewItem =~ /.*,DATASET,.*/",
					"group": "inline"
				},
				{
					"command": "PowerBIDataset.createKernel",
					"when": "view == PowerBIWorkspaces && viewItem =~ /.*,DATASET,.*/ && viewItem =~ /.*,CREATE_KERNEL,.*/",
					"group": "inline"
				},
				{
					"command": "PowerBIDataset.removeKernel",
					"when": "view == PowerBIWorkspaces && viewItem =~ /.*,DATASET,.*/ && viewItem =~ /.*,REMOVE_KERNEL,.*/",
					"group": "inline"
				},
				{
					"command": "PowerBIDataset.takeOver",
					"when": "view == PowerBIWorkspaces && viewItem =~ /.*,DATASET,.*/"
				},
				{
					"command": "PowerBIDataset.updateAllParameters",
					"when": "view == PowerBIWorkspaces && viewItem =~ /.*,DATASET,.*/"
				},
				{
					"command": "PowerBIDataset.cancelRefresh",
					"when": "view == PowerBIWorkspaces && viewItem =~ /.*,REFRESH,.*/ && viewItem =~ /.*,CANCEL,.*/",
					"group": "inline"
				},
				{
					"command": "PowerBIDatasetParameter.update",
					"when": "view == PowerBIWorkspaces && viewItem =~ /.*,PARAMETER,.*/",
					"group": "inline"
				},
				{
					"command": "PowerBIReport.takeOver",
					"when": "view == PowerBIWorkspaces && viewItem =~ /.*,REPORT,.*/"
				},
				{
					"command": "PowerBIReport.delete",
					"when": "view == PowerBIWorkspaces && viewItem =~ /.*,REPORT,.*/",
					"group": "inline"
				},
				{
					"command": "PowerBIReport.clone",
					"when": "view == PowerBIWorkspaces && viewItem =~ /.*,REPORT,.*/"
				},
				{
					"command": "PowerBIReport.rebind",
					"when": "view == PowerBIWorkspaces && viewItem =~ /.*,REPORT,.*/"
				},
				{
					"command": "PowerBIReport.updateContent",
					"when": "view == PowerBIWorkspaces && viewItem =~ /.*,REPORT,.*/"
				},
				{
					"command": "PowerBIDataflow.delete",
					"when": "view == PowerBIWorkspaces && viewItem =~ /.*,DATAFLOW,.*/",
					"group": "inline"
				},
				{
					"command": "PowerBIDataflow.refresh",
					"when": "view == PowerBIWorkspaces && viewItem =~ /.*,DATAFLOW,.*/",
					"group": "inline"
				},
				{
					"command": "PowerBIDashboard.delete",
					"when": "view == PowerBIWorkspaces && viewItem =~ /.*,DATAFLOW,.*/",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts",
		"test": "vscode-test-web --extensionDevelopmentPath=. --extensionTestsPath=dist/web/extension.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "^12.12.0",
		"@types/rimraf": "^2.0.5",
		"@types/vscode": "^1.68.0",
		"eslint": "^8.19.0",
		"form-data": "^4.0.0",
		"glob": "^8.0.3",
		"https-agent": "^1.0.0",
		"https-proxy-agent": "^5.0.1",
		"mocha": "^10.0.0",
		"native-ext-loader": "^2.3.0",
		"node-fetch": "^3.2.10",
		"process": "^0.11.10",
		"ts-loader": "^9.3.1",
		"typescript": "^4.7.4",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0"
	}
}
